	#ex_1 : Write a program to calculate s=1+2+3+....+n.
	# Print s
	clear
	tput cup 13 10 # To move cursor to 13 -th row and 10-th column
	echo -n "Enter value of n(1-20)="
	read n
	s=0
	c=1
		while [ $c -le $n ]
		do
		s=`expr $s + $c`
		c=`expr $c + 1`
		done
	tput cup 14 10
	echo "Sum of $n numbers=$s"
	#end of program

	

