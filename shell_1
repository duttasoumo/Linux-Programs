	#shell_1 : Write a program to input roll,total.
	#Calculate grade. Print roll, total, grade
	clear
	echo
	echo -n "Enter number of students(1-10)="
	read n
	c=1
	while [ $c -le $n ]
	do
	echo -n "Enter Roll(1-100)="
	read roll
	echo -n "Enter Total(0-100)="
	read total
	#To calculate grade
		if [ $total -ge 70 ]
		then
		grade="A"
			elif [ $total -ge 60 ]
			then
			grade="B"
				elif [ $total -ge 50 ]
				then
				grade="C"
					 elif [ $total -ge 40 ]
					then
					grade="D"
					else
					grade="F"
		fi
   echo " Student no=$c Roll=$roll  Total=$total  Grade=$grade"
	c=`expr $c + 1`
	done
	#end of program












